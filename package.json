        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-jsonschema": "^1.1.6",
        "glob": "^7.1.6",
        "jsonwebtoken": "^9.0.0",
        "knex": "^0.21.1",
        "knexnest": "^1.0.0",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
